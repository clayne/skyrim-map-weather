{
	"configurePresets": [
		{
			"binaryDir": "${sourceDir}/build",
			"cacheVariables": {
				"CMAKE_BUILD_TYPE": {
					"type": "STRING",
					"value": "Release"
				}
			},
			"errors": {
				"deprecated": true
			},
			"hidden": true,
			"name": "cmake-dev",
			"warnings": {
				"deprecated": true,
				"dev": true
			}
		},
		{
			"cacheVariables": {
				"CMAKE_TOOLCHAIN_FILE": {
					"type": "STRING",
					"value": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake"
				}
			},
			"hidden": true,
			"name": "vcpkg"
		},
		{
			"cacheVariables": {
				"CMAKE_MSVC_RUNTIME_LIBRARY": {
					"type": "STRING",
					"value": "MultiThreaded$<$<CONFIG:Debug>:Debug>"
				},
				"VCPKG_TARGET_TRIPLET": {
					"type": "STRING",
					"value": "x64-windows-static"
				}
			},
			"hidden": true,
			"name": "windows"
		},
		{
			"cacheVariables": {
				"CMAKE_CXX_FLAGS": "/EHsc /MP /W4 /WX"
			},
			"generator": "Visual Studio 17 2022",
			"toolset": "v143",
			"hidden": true,
			"name": "msvc"
		},
		{
			"binaryDir": "${sourceDir}/build/pre-ae",
			"cacheVariables": {
				"BUILD_SKYRIMSE": true
			},
			"inherits": [
				"cmake-dev",
				"vcpkg",
				"windows",
				"msvc"
			],
			"name": "pre-ae"
		},
		{
			"binaryDir": "${sourceDir}/build/post-ae",
			"cacheVariables": {
				"BUILD_SKYRIMAE": true
			},
			"inherits": [
				"cmake-dev",
				"vcpkg",
				"windows",
				"msvc"
			],
			"name": "post-ae"
		},
		{
			"binaryDir": "${sourceDir}/build/vr",
			"cacheVariables": {
				"BUILD_SKYRIMVR": true
			},
			"inherits": [
				"cmake-dev",
				"vcpkg",
				"windows",
				"msvc"
			],
			"name": "vr"
		}
	],
	"version": 3
}
